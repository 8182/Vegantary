 <div class="wrapper">
    <form class="form-signin">       
      <h2 class="form-signin-heading text-center">Registrate!</h2>
      <div class="container">

      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>
        
        <div class="form-group text text-align" >
          <%= f.label :email, "Correo electronico" %>
          <%= f.email_field :email, autofocus: true, class: "form-control" %>
        </div>

        <div class="form-group">
          <%= f.label :password, "ContraseÃ±a" %>
          <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
        </div>

        <div class="form-group">
          <%= f.label :password_confirmation, "Repite tu contraseÃ±a" %>
          <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control" %>
        </div>

        <div class="actions">
          <%= f.submit "Create my account", class: "btn btn-success" %>
        </div>
      <% end %>
      
    </div>
  </div>
</div>

